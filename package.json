{
  "name": "create-react-spa",
  "version": "1.0.0",
  "description": "A bare-bones skeleton for starting a single-page React app",
  "author": "Michael Allen Warner",
  "engines": {
    "node": "^12.3.1"
  },
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
